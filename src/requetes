Q04

SELECT t0.CODEPROJ, t0.DATEDEP, t0.DATEFINP, t0.DESCPROJ, t0.MONTANTDEVIS, t0.LECLIENT_NUMCLI FROM PROJET t0 WHERE ((SELECT COUNT(t1.ID) FROM AFFECTATION t1 WHERE (t1.LEPROJET_CODEPROJ = t0.CODEPROJ)) = ?)
	bind => [0]
SELECT NUMCLI, ADRCLI, NOMCLI FROM CLIENT WHERE (NUMCLI = ?)
	bind => [1001]


Q05

SELECT CODEPROJ, DATEDEP, DATEFINP, DESCPROJ, MONTANTDEVIS, LECLIENT_NUMCLI FROM PROJET WHERE (MONTANTDEVIS > ?)
	bind => [500.0]
SELECT NUMCLI, ADRCLI, NOMCLI FROM CLIENT WHERE (NUMCLI = ?)
	bind => [1003]
SELECT NUMCLI, ADRCLI, NOMCLI FROM CLIENT WHERE (NUMCLI = ?)
	bind => [1001]
SELECT NUMCLI, ADRCLI, NOMCLI FROM CLIENT WHERE (NUMCLI = ?)
	bind => [1002]

Q06

SELECT t0.CODEPROJ, t0.DATEDEP, t0.DATEFINP, t0.DESCPROJ, t0.MONTANTDEVIS, t0.LECLIENT_NUMCLI FROM PROJET t0 WHERE ((t0.MONTANTDEVIS > ?) AND ((SELECT COUNT(t1.ID) FROM AFFECTATION t1 WHERE (t1.LEPROJET_CODEPROJ = t0.CODEPROJ)) > ?))
	bind => [500.0, 2]
SELECT NUMCLI, ADRCLI, NOMCLI FROM CLIENT WHERE (NUMCLI = ?)
	bind => [1003]
SELECT NUMCLI, ADRCLI, NOMCLI FROM CLIENT WHERE (NUMCLI = ?)
	bind => [1002]


Q07

SELECT CODEPROJ, DATEDEP, DATEFINP, DESCPROJ, MONTANTDEVIS, LECLIENT_NUMCLI FROM PROJET WHERE (CODEPROJ = ?)
	bind => [Alpha]
SELECT NUMCLI, ADRCLI, NOMCLI FROM CLIENT WHERE (NUMCLI = ?)
	bind => [1001]
SELECT ID, DATEAFF, PILOTE, LEPROJET_CODEPROJ, LESALARIE_NUMSAL FROM AFFECTATION WHERE (LEPROJET_CODEPROJ = ?)
	bind => [Alpha]
SELECT NUMSAL, NOMSAL, SALAIRE, SEXE, CODEFONCT FROM SALARIE WHERE (NUMSAL = ?)
	bind => [101]
SELECT CODEFONCT, LIBFONCT FROM FONCTION WHERE (CODEFONCT = ?)
	bind => [SLAM]
SELECT NUMSAL, NOMSAL, SALAIRE, SEXE, CODEFONCT FROM SALARIE WHERE (NUMSAL = ?)
	bind => [102]

Q08

SELECT t1.CODEPROJ, t1.DATEDEP, t1.DATEFINP, t1.DESCPROJ, t1.MONTANTDEVIS, t1.LECLIENT_NUMCLI, t0.ID, t0.DATEAFF, t0.PILOTE, t0.LEPROJET_CODEPROJ, t0.LESALARIE_NUMSAL, t2.NUMCLI, t2.ADRCLI, t2.NOMCLI FROM AFFECTATION t0, CLIENT t2, PROJET t1 WHERE ((t1.CODEPROJ = ?) AND ((t0.LEPROJET_CODEPROJ = t1.CODEPROJ) AND (t2.NUMCLI = t1.LECLIENT_NUMCLI)))
	bind => [Alpha]
SELECT NUMSAL, NOMSAL, SALAIRE, SEXE, CODEFONCT FROM SALARIE WHERE (NUMSAL = ?)
	bind => [101]
SELECT CODEFONCT, LIBFONCT FROM FONCTION WHERE (CODEFONCT = ?)
	bind => [SLAM]
SELECT NUMSAL, NOMSAL, SALAIRE, SEXE, CODEFONCT FROM SALARIE WHERE (NUMSAL = ?)
	bind => [102]


Q09

SELECT t0.CODEPROJ, t0.DATEDEP, t0.DATEFINP, t0.DESCPROJ, t0.MONTANTDEVIS, t0.LECLIENT_NUMCLI FROM PROJET t0 WHERE (t0.MONTANTDEVIS = (SELECT MAX(t1.MONTANTDEVIS) FROM PROJET t1))
SELECT NUMCLI, ADRCLI, NOMCLI FROM CLIENT WHERE (NUMCLI = ?)
	bind => [1002]


Q10

SELECT SUM(MONTANTDEVIS) FROM PROJET

Q11

SELECT SUM(MONTANTDEVIS) FROM PROJET WHERE (LECLIENT_NUMCLI = ?)
	bind => [1001]

Q12

SELECT DISTINCT t1.NUMSAL, t1.NOMSAL, t1.SALAIRE, t1.SEXE, t1.CODEFONCT, t0.CODEFONCT, t0.LIBFONCT, t2.ID, t2.DATEAFF, t2.PILOTE, t2.LEPROJET_CODEPROJ, t2.LESALARIE_NUMSAL FROM SALARIE t1 LEFT OUTER JOIN AFFECTATION t2 ON (t2.LESALARIE_NUMSAL = t1.NUMSAL), FONCTION t0 WHERE (((t1.SEXE = ?) AND (t1.SALAIRE BETWEEN ? AND ?)) AND (t0.CODEFONCT = t1.CODEFONCT))
	bind => [M, 500.0, 1000.0]

